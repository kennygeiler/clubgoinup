<% if current_user.promotors == nil %>
  <% redirect_to new_quote_path %>
<% else %>
  <% @quotes.each do |quote| %>
    <% if current_user.promotors.first.location.downcase == quote.location.downcase %>
      <%= link_to quote.user.name, quote_path(quote.id) %>
      <%= quote.date %>
      <%= quote.location %>
      <%= quote.venues %> <br>
    <% end %>
  <% end %>
<% end %>